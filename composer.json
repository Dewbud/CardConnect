{
    "name": "dewbud/cardconnect",
    "description": "PHP adapter for CardConnect CardPointe API",
    "version": "1.1.0",
    "keywords": [
        "cardconnect",
        "cardpointe"
    ],
    "homepage": "https://github.com/Dewbud/CardConnect",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dewbud",
            "email": "jalfonso1134@gmail.com"
        }
    ],
    "scripts": {
        "test": "phpunit",
        "lint": [
            "php-cs-fixer fix --config ./.php_cs"
        ]
    },
    "autoload": {
        "psr-4": {
            "Dewbud\\CardConnect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dewbud\\CardConnect\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "friendsofphp/php-cs-fixer": "^2.15"
    }
}